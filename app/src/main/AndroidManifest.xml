<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="h_kang.myapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/me"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityLearn">
            <intent-filter>

                <!-- action android:name="h_kang.myapp.ACTION_START"/ -->
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:scheme="http" />
            </intent-filter>
        </activity>
        <activity android:name=".util.Main2Activity" />
        <activity android:name=".SimpleListView" />
        <activity android:name=".FragmentActivity"></activity>
    </application>

</manifest>